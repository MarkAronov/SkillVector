{
	"buildCommand": "bun run build",
	"outputDirectory": "dist",
	"installCommand": "bun install",
	"framework": "vite",
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/index.html"
		}
	],
	"redirects": [
		{
			"source": "/resources",
			"destination": "/documentation",
			"statusCode": 301
		},
		{
			"source": "/api",
			"destination": "/documentation",
			"statusCode": 301
		},
		{
			"source": "/api/(.*)",
			"destination": "/documentation/$1",
			"statusCode": 301
		},
		{
			"source": "/sdk",
			"destination": "/documentation#sdk",
			"statusCode": 301
		}
	],
	"headers": [
		{
			"source": "/assets/(.*)",
			"headers": [
				{
					"key": "Cache-Control",
					"value": "public, max-age=31536000, immutable"
				}
			]
		}
	]
}
